{
  "expo": {
    "name": "LaterBox",
    "slug": "laterbox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "laterbox",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "url": "https://u.expo.dev/c87db489-f44f-45ff-bec0-2c5e16b0c920"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xcore.laterbox",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#83ADAB",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.xcore.laterbox",
      "versionCode": 1,
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow LaterBox to use the camera to scan QR codes for the web client."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#83ADAB"
        }
      ],
      [
        "expo-splash-screen",
        {
          "ios": {
            "backgroundColor": "#83ADAB",
            "image": "./assets/images/android-icon-foreground.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "dark": {
              "backgroundColor": "#0f172a",
              "image": "./assets/images/android-icon-foreground.png"
            }
          },
          "android": {
            "backgroundColor": "#83ADAB",
            "image": "./assets/images/android-icon-foreground.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "dark": {
              "backgroundColor": "#83ADAB",
              "image": "./assets/images/android-icon-foreground.png"
            }
          }
        }
      ],
      "expo-font",
      "@react-native-community/datetimepicker",
      "expo-sqlite",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow LaterBox to access your location for sharing."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow LaterBox to access your contacts for sharing."
        }
      ],
      "expo-video",
      [
        "expo-share-intent",
        {
          "iosActivationRules": {
            "NSExtensionActivationSupportsText": true,
            "NSExtensionActivationSupportsWebURLWithMaxCount": 1,
            "NSExtensionActivationSupportsWebPageWithMaxCount": 1,
            "NSExtensionActivationSupportsImageWithMaxCount": 10,
            "NSExtensionActivationSupportsMovieWithMaxCount": 5,
            "NSExtensionActivationSupportsFileWithMaxCount": 5
          },
          "androidIntentFilters": [
            "text/*",
            "image/*",
            "video/*",
            "audio/*",
            "application/*"
          ],
          "androidMultiIntentFilters": [
            "image/*",
            "video/*"
          ],
          "iosAppGroupIdentifier": "group.com.xcore.laterbox.shared",
          "iosShareExtensionBundleIdentifier": "com.xcore.laterbox.shareextension"
        }
      ],
      ["expo-build-properties", {
        "ios": {
          "deploymentTarget": "16.0"
        }
      }],
      ["./plugins/withDeploymentTarget", {
        "deploymentTarget": "16.0",
        "developmentTeam": "8S76DB4279"
      }]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c87db489-f44f-45ff-bec0-2c5e16b0c920"
      }
    },
    "owner": "mniac"
  }
}
