{
  "expo": {
    "name": "LaterBox",
    "slug": "laterbox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "laterbox",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xcore.laterbox",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.xcore.laterbox",
      "versionCode": 1
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#3b82f6"
        }
      ],
      [
        "expo-splash-screen",
        {
          "ios": {
            "backgroundColor": "#ffffff",
            "image": "./assets/images/android-icon-foreground.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "dark": {
              "backgroundColor": "#0f172a",
              "image": "./assets/images/android-icon-foreground-dark.png"
            }
          },
          "android": {
            "backgroundColor": "#ffffff",
            "image": "./assets/images/android-icon-foreground.png",
            "imageWidth": 200,
            "resizeMode": "contain",
            "dark": {
              "backgroundColor": "#0f172a",
              "image": "./assets/images/android-icon-foreground-dark.png"
            }
          }
        }
      ],
      "expo-font",
      "@react-native-community/datetimepicker",
      "expo-sqlite",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow LaterBox to access your location for sharing."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow LaterBox to access your contacts for sharing."
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c8649cab-209c-4b68-98a1-9a7a2fbba547"
      }
    },
    "owner": "mniac"
  }
}
